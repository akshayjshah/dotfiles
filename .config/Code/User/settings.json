{
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "window.zoomLevel": 1,
    "editor.fontSize": 16,
    "editor.fontFamily": "'Fira Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.accessibilitySupport": "off",
    "editor.fontLigatures": true,
    "editor.formatOnSave": true,
    "files.exclude": {
        "**/.tmp": true
    },
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "files.watcherExclude": {
        "**/.tmp/**": true
    },
    "workbench.colorTheme": "Visual Studio Dark",
    "window.restoreWindows": "all",
    "window.newWindowDimensions": "inherit",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.shell.linux": "/bin/zsh",
    "go.formatTool": "goimports",
    "go.installDependenciesWhenBuilding": false,
    "go.useCodeSnippetsOnFunctionSuggestWithoutType": true,
    "markdown.preview.fontFamily": "'Fira Sans', -apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', 'HelveticaNeue-Light', 'Ubuntu', 'Droid Sans', sans-serif",
    "markdown.preview.fontSize": 16,
    "python.formatting.provider": "black",
    "python.linting.flake8Enabled": true,
    "python.unitTest.pyTestEnabled": true,
    "searchRipgrep.enable": true,
    "vim.easymotionMarkerFontSize": "16",
    "vim.leader": ",",
    "vim.statusBarColorControl": true,
    "vim.useSystemClipboard": true,
    "yaml.format.enable": true,
    "yaml.format.proseWrap": "always",
    "workbench.colorCustomizations": {
        "statusBar.background": "#005f5f",
        "statusBar.noFolderBackground": "#005f5f",
        "statusBar.debuggingBackground": "#005f5f"
    },
    "keyboard.dispatch": "keyCode"
}
